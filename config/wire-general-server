
##
## wire-general-server
##

WG_CLOUDVPN_SERVER_LISTEN_PORT="51892"

## avoid using 192.168.122 NETWORK_BASE as it is used by virbr0

WG_CLOUDVPN_SERVER_NETWORK_BASE="192.168.110"
WG_CLOUDVPN_SERVER_NETWORK_MASK="24"
WG_CLOUDVPN_SERVER_IP_END="10"

## server dnscrypt-proxy flag
SETUP_DNSCRYPT_PROXY_BLOCKING="YES"

## client config file in config/<name>-client
WG_CLOUDVPN_SERVER_CLIENT_LIST="wire-general"
